{
  "logdir": "<%= logdir %>",
  "github_client_id": "<%= github_client_id %>",
  "github_client_secret": "<%= github_client_secret %>",
  "statsd": "ops1.holderdeord.no:8125",
  "basic_auth": "<%= basic_auth %>",
  "projects": {
    "holderdeord/hdo-site#master": {
      "directory": "<%= appdir %>",
      "token": "<%= token %>",
      "timeout": 180,
      "environment": {
        "HIPCHAT_API_TOKEN": "<%= hipchat_api_token %>"
      },
      "command": [
        "cap",
        "-S",
        "revision=%{sha}",
        "staging",
        "deploy:web:disable",
        "deploy:migrations",
        "deploy:web:enable",
        "deploy:cleanup"
      ]
    }
  }
}